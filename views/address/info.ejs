                                    <% } else { %>
                                    <%= organization.attributes.attribute[i].value %>
                                    <% } %>
                                </td>
                            </tr>
                            <% } %>
                        </table>
                    </div>
                </div>
            </div>
        </div>
    </div>

    <div class="row">
        <div class="panel-group" id="accordion" role="tablist" aria-multiselectable="true">
